    "version": "0.2.0",
    "configurations": [

        {
            "name": "training lgbm",
            "type": "python",
            "request": "launch",
            "program": "dl4tsf/train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": ["model=lgbm", "model/features=default_idf_ferro", "train=train_lgbm","dataset=idf_ferro", "feature_engineering=default_idf_mobilite"],

        } ,

        {
            "name": "training informer",
            "type": "python",
            "request": "launch",
            "program": "dl4tsf/train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "env": {
                "PYTORCH_ENABLE_MPS_FALLBACK": "1"
              },
            "args": ["model=informer", "dataset=idf_ferro"],

        } ,
        {
            "name": "training tft",
            "type": "python",
            "request": "launch",
            "program": "dl4tsf/train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "env": {
                "PYTORCH_ENABLE_MPS_FALLBACK": "1"
              },
            "args": ["model=tft", "dataset=enedis"],

        } ,
        {
            "name": "inference tft",
            "type": "python",
            "request": "launch",
            "program": "dl4tsf/inference.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "env": {
                "PYTORCH_ENABLE_MPS_FALLBACK": "1"
              },
            "args": ["logging=azure"],

        } ,
    ]
}
